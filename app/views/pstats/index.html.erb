

<div class="container" id="containerb">


<p class="attn">Listing Payment Status Types</p>

<table class="table table-condensed">
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @pstats.each do |pstat| %>
      <tr>
        <td><%= pstat.name %></td>
        <td><%= link_to 'Show', pstat %></td>
        <td><%= link_to 'Edit', edit_pstat_path(pstat) %></td>
        <td><%= link_to 'Destroy', pstat, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Payment Status', new_pstat_path %>

</div>